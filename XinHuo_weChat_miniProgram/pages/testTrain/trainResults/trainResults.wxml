<!-- 答题结果 -->
<view class="page">
    <view class="page_title">答题结果</view>
    <view class="padding">
        <view class='page-head'>
            <image src="http://evinci.oss-cn-hangzhou.aliyuncs.com/evinci/主页-结业测试-考试结果.jpg"></image>
            <view class='page-score'>
                <text class="score-num text-red text-bold">{{totalScore}}分</text>
            </view>
        </view>
        <view class='page-footer'>
            <view class='score_detail'>
                <view class='detail1'>
                    <view class="detail_number1">{{totalNumber-wrong}}题</view>
                    <view class="detail_content">
                        答对</view>
                </view>
                <view class='detail2'>
                    <view class="detail_number2">{{wrong}}题</view>
                    <view class="detail_content">
                        答错</view>
                </view>
                <view class='detail3'>
                    <view class="detail_number3">{{zql}}%</view>
                    <view class="detail_content">
                        正确率</view>
                </view>
            </view>
            <view class="flex padding flex-direction">
                <button bindtap="toIndex" hover-class="other-button-hover" class="cu-btn line-red round lg margin-top" style="margin-top:100rpx;width: 450rpx;height: 80rpx;margin-bottom: 20rpx;border-radius: 40rpx;box-shadow: 3px 5px 5px #ccc;background-color: #D8412B;color: white;"> 返回首页 </button>
            </view>
        </view>
    </view>
</view>